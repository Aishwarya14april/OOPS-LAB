#include<iostream>
using namespace std;
void print(char c)
{
    for(int i=1;i<=80;i++)
    {
        cout<<c<<" ";
    }
    cout<<"\n";
}
void print()
{
    for(int i=1;i<=80;i++)
    {
        cout<<"* ";
    }
    cout<<"\n";
}
int main()
{
    int n;
    char c;
    cout<<"Enter the character: ";
    cin>>c;
    print(c);
    print();
    return 0;
}
